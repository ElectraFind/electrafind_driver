{
  "expo": {
    "name": "electrafind_driver",
    "scheme": "electrafind_driver",
    "slug": "electrafind_driver",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.company.electrafind_driver",
      "ios.config.googleMapsApiKey": "AIzaSyDbkBRNcFZ-9PqE10_PdhhwQMcZaZlEItU"
      
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow ElectraFind to use your location."
        }
      
    ]
  }
}
